</head>
<body class="dashboard">
    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    <nav class="site-navbar navbar navbar-default navbar-fixed-top navbar-mega" role="navigation">

        <div class="navbar-header">
            <button type="button" class="navbar-toggle hamburger hamburger-close navbar-toggle-left hided"
                    data-toggle="menubar">
                <span class="sr-only">Toggle navigation</span>
                <span class="hamburger-bar"></span>
            </button>
            <button type="button" class="navbar-toggle collapsed" data-target="#site-navbar-collapse"
                    data-toggle="collapse">
                <i class="icon wb-more-horizontal" aria-hidden="true"></i>
            </button>
            <button type="button" class="navbar-toggle collapsed" data-target="#site-navbar-search"
                    data-toggle="collapse">
                <span class="sr-only">Toggle Search</span>
                <i class="icon wb-search" aria-hidden="true"></i>
            </button>
            <div class="navbar-brand navbar-brand-center" data-toggle="gridmenu">
                <div class="navbar-brand-logo"></div>
            </div>
        </div>

        <div class="navbar-container container-fluid">
            <!-- Navbar Collapse -->
            <div class="collapse navbar-collapse navbar-collapse-toolbar" id="site-navbar-collapse">
                <!-- Navbar Toolbar -->
                <ul class="nav navbar-toolbar">
                    <li class="hidden-float" id="toggleMenubar">
                        <a data-toggle="menubar" href="#" role="button">
                            <i class="icon hamburger hamburger-arrow-left">
                                <span class="sr-only">Toggle menubar</span>
                                <span class="hamburger-bar"></span>
                            </i>
                        </a>
                    </li>

                </ul>
                <!-- End Navbar Toolbar -->
                <!-- Navbar Toolbar Right -->
                <ul class="nav navbar-toolbar navbar-right navbar-toolbar-right">
                    <li>
                        <a href="/Home/Help" title="Help">
                            <i class="icon wb-help-circle" aria-hidden="true"></i>
                        </a>
                    </li>
                </ul>
                <!-- End Navbar Toolbar Right -->
            </div>
            <!-- End Navbar Collapse -->
        </div>
    </nav>
    <div class="site-menubar">
        <div class="site-menubar-body">
            <div>
                <div>
                    <ul class="site-menu">
                        @if (User.IsInRole("Global Administrator"))
                        {
                            <li class="site-menu-category">General</li>

                            <li class="site-menu-item @Html.IsSelected(actions: "Index", controllers: "Home")">
                                <a href='@Url.Action("Index", "Home")' data-slug="dashboard">
                                    <i class="site-menu-icon wb-home" aria-hidden="true"></i>
                                    <span class="site-menu-title">Home</span>
                                </a>
                            </li>

                            <li class="site-menu-category">User Management</li>
                            

                            <li class="site-menu-item">
                                <a href="@Url.Action("Index", "Accounts")" data-slug="dashboard">
                                    <i class="site-menu-icon fa fa-user" aria-hidden="true"></i>
                                    <span class="site-menu-title">Users</span>
                                </a>
                            </li>

                            <li class="site-menu-item">
                                <a href="@Url.Action("Index", "AppRoles")" data-slug="dashboard">
                                    <i class="site-menu-icon wb-star" aria-hidden="true"></i>
                                    <span class="site-menu-title">Roles</span>
                                </a>
                            </li>

                            <li class="site-menu-item">
                                <a href="@Url.Action("Index", "Groups")" data-slug="dashboard">
                                    <i class="site-menu-icon wb-users" aria-hidden="true"></i>
                                    <span class="site-menu-title">Groups</span>
                                </a>
                            </li>

                            <li class="site-menu-category">Device Management</li>

                            <li class="site-menu-item">
                                <a href="@Url.Action("Index", "Devices")" data-slug="dashboard">
                                    <i class="site-menu-icon wb-mobile" aria-hidden="true"></i>
                                    <span class="site-menu-title">Devices</span>
                                </a>
                            </li>


                            <li class="site-menu-item">
                                <a href="@Url.Action("Index", "GroupDevices")" data-slug="dashboard">
                                    <i class="site-menu-icon fa fa-arrows-alt" aria-hidden="true"></i>
                                    <span class="site-menu-title">Group Devices</span>
                                </a>
                            </li>


                            <li class="site-menu-item">
                                <a href="@Url.Action("Index", "VehicleTypes")" data-slug="dashboard">
                                    <i class="site-menu-icon wb-mobile" aria-hidden="true"></i>
                                    <span class="site-menu-title">Vehicle Types</span>
                                </a>
                            </li>
                        }

                        @if (User.IsInRole("Client Administrator"))
                        {
                            <li class="site-menu-category">General</li>

                            <li class="site-menu-item @Html.IsSelected(actions: "Index", controllers: "Home")">
                                <a href='@Url.Action("Index", "Home")' data-slug="dashboard">
                                    <i class="site-menu-icon wb-home" aria-hidden="true"></i>
                                    <span class="site-menu-title">Home</span>
                                </a>
                            </li>

                            <li class="site-menu-item @Html.IsSelected(actions: "About", controllers: "Home")">
                                <a href='@Url.Action("About", "Home")' data-slug="dashboard">
                                    <i class="site-menu-icon fa fa-info-circle" aria-hidden="true"></i>
                                    <span class="site-menu-title">About</span>
                                </a>
                            </li>

                            <li class="site-menu-item @Html.IsSelected(actions: "Contact", controllers: "Home")">
                                <a href='@Url.Action("Contact", "Home")' data-slug="dashboard">
                                    <i class="site-menu-icon fa fa-envelope" aria-hidden="true"></i>
                                    <span class="site-menu-title">Contact</span>
                                </a>
                            </li>

                            <li class="site-menu-category">Tracking</li>

                            <li class="site-menu-item @Html.IsSelected(actions: "Index", controllers: "Vehicles")">
                                <a href="@Url.Action("Index", "Vehicles")" data-slug="dashboard">
                                    <i class="site-menu-icon fa fa-car" aria-hidden="true"></i>
                                    <span class="site-menu-title">Vehicles</span>
                                </a>
                            </li>

                            <li class="site-menu-item @Html.IsSelected(actions: "Index", controllers: "Drivers")">
                                <a href="@Url.Action("Index", "Drivers")" data-slug="dashboard">
                                    <i class="site-menu-icon fa fa-users" aria-hidden="true"></i>
                                    <span class="site-menu-title">Drivers</span>
                                </a>
                            </li>

                            <li class="site-menu-item @Html.IsSelected(actions: "Index", controllers: "Routes")">
                                <a href="@Url.Action("Index", "Routes")" data-slug="dashboard">
                                    <i class="site-menu-icon wb-map" aria-hidden="true"></i>
                                    <span class="site-menu-title">Routes</span>
                                </a>
                            </li>


                            <li class="site-menu-item @Html.IsSelected(actions: "Index", controllers: "Invoices")">
                                <a href="@Url.Action("Index", "Invoices")" data-slug="dashboard">
                                    <i class="site-menu-icon wb-table" aria-hidden="true"></i>
                                    <span class="site-menu-title">Invoices</span>
                                </a>
                            </li>


                            <li class="site-menu-item">
                                <a href="javascript:void(0)" data-slug="dashboard">
                                    <i class="site-menu-icon wb-globe" aria-hidden="true"></i>
                                    <span class="site-menu-title">Map</span>
                                </a>
                            </li>


                            <li class="site-menu-item">
                                <a href="javascript:void(0)" data-slug="dashboard">
                                    <i class="site-menu-icon wb-stats-bars" aria-hidden="true"></i>
                                    <span class="site-menu-title">Stats</span>
                                </a>
                            </li>
                        }
                    </ul>

                </div>
            </div>
        </div>

        <div class="site-menubar-footer">

            <a href="/Home/Logout" data-placement="top" data-toggle="tooltip" data-original-title="Logout" style="width: 100%;">
                <span class="icon wb-power" aria-hidden="true"></span>
            </a>
        </div>
    </div>
    


    <div class="page">
        <div class="page-content padding-30 container-fluid">
            <div class="row" data-plugin="matchHeight" data-by-row="true">
                @RenderBody()
            </div>
        </div>
    </div>

    <footer class="site-footer">
        <span class="site-footer-legal">© 2016 0xdeadc0de</span>
        <div class="site-footer-right">
            Parić M., Polić M., Popović A., Ramić B.
        </div>
    </footer>


    <script src="https://maps.googleapis.com/maps/api/js?sensor=true"></script>

    @Scripts.Render("~/bundles/core-scripts")
    @Scripts.Render("~/bundles/plugin-scripts")


    @Scripts.Render("~/bundles/page-scripts")

    <script>
        $(document).ready(function($) {
            Site.run();
        });
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>
