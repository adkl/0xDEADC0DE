<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="author" content="0xDEADC0DE">

    <title>@ViewBag.Title - fleet-tracker</title>

    <link rel="apple-touch-icon" href="../assets/images/apple-touch-icon.png">
    <link rel="shortcut icon" href="../assets/images/favicon.ico">

    @Styles.Render("~/bundles/core-styles-css")
    @Styles.Render("~/bundles/core-fonts-css")
    @Styles.Render("~/bundles/roboto-font-css")

    @Scripts.Render("~/bundles/header-scripts")

    <script>
        Breakpoints();
    </script>

